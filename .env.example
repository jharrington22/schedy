# Copy to .env and fill values.
# Generate COOKIE_* via: go run . keys

LISTEN_ADDR=:8080
BASE_URL=http://localhost:8080

DATABASE_URL=postgres://resy:resy@db:5432/resy?sslmode=disable

# base64 values
COOKIE_HASH_KEY=
COOKIE_BLOCK_KEY=

SCHED_POLL_SECONDS=2
RESY_BIN=resy

# resy-cli stores creds on disk; in compose we mount ./\.resy -> /data/resy and set HOME=/data/resy
